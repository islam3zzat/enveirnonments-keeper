{
    "extends": [
        "tslint:latest",
        "tslint-react"
    ],
    "rules": {
        "jsx-wrap-multiline": false,
        "jsx-no-multiline-js": false,
        "arrow-parens": false,
        "jsx-no-lambda": false,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "trailing-comma": false,
        "variable-name": false,
        "no-submodule-imports": false,
        "no-implicit-dependencies": false
    }
}